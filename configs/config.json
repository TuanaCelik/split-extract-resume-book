{
  "extract": {
    "json_schema": {
      "type": "object",
      "properties": {
        "full_name": {
          "type": ["string", "null"],
          "description": "The candidate's full name"
        },
        "email": {
          "type": ["string", "null"],
          "description": "Email address"
        },
        "phone": {
          "type": ["string", "null"],
          "description": "Phone number"
        },
        "linkedin_url": {
          "type": ["string", "null"],
          "description": "LinkedIn profile URL"
        },
        "graduation_year": {
          "type": ["integer", "null"],
          "description": "Expected or actual graduation year"
        },
        "degree": {
          "type": ["string", "null"],
          "description": "Degree type (e.g., Bachelor of Science, MBA, PhD)"
        },
        "major": {
          "type": ["string", "null"],
          "description": "Field of study or major"
        },
        "university": {
          "type": ["string", "null"],
          "description": "Name of the university or college"
        },
        "gpa": {
          "type": ["number", "null"],
          "description": "Grade point average if listed"
        },
        "work_experience": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "company": {
                "type": ["string", "null"],
                "description": "Company or organization name"
              },
              "title": {
                "type": ["string", "null"],
                "description": "Job title or role"
              },
              "start_date": {
                "type": ["string", "null"],
                "description": "Start date of employment"
              },
              "end_date": {
                "type": ["string", "null"],
                "description": "End date of employment, or 'Present' if current"
              },
              "description": {
                "type": ["string", "null"],
                "description": "Brief description of responsibilities and achievements"
              }
            },
            "required": ["company", "title", "start_date", "end_date", "description"]
          },
          "description": "List of work experiences in reverse chronological order"
        },
        "skills": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Technical and professional skills"
        },
        "certifications": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Professional certifications or licenses"
        },
        "languages": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Languages spoken"
        }
      },
      "required": [
        "full_name",
        "email",
        "phone",
        "linkedin_url",
        "graduation_year",
        "degree",
        "major",
        "university",
        "gpa",
        "work_experience",
        "skills",
        "certifications",
        "languages"
      ]
    },
    "settings": {
      "extraction_mode": "PREMIUM",
      "system_prompt": null,
      "citation_bbox": true,
      "use_reasoning": false,
      "cite_sources": true,
      "confidence_scores": true
    }
  },
  "split": {
    "categories": [
      {
        "name": "resume",
        "description": "An individual resume or CV containing a person's contact information, education, work experience, and skills"
      },
      {
        "name": "curriculum",
        "description": "Academic curriculum pages, course listings, program descriptions, or degree requirements"
      },
      {
        "name": "cover_page",
        "description": "Title page, table of contents, introduction, or other front matter that is not a resume"
      }
    ],
    "settings": {
      "splitting_strategy": {
        "allow_uncategorized": false
      }
    }
  },
  "classify": {
    "rules": [],
    "settings": {}
  },
  "parse": {
    "settings": {
      "tier": "agentic",
      "version": "latest"
    }
  }
}
